@model QuanLyXe.Models.LOAISANPHAM
@{
    ViewBag.Title = "Thêm mới loại xe";
    Layout = "~/Views/Shared/_LayoutChothue.cshtml";
}
<!-- End Navbar -->
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-8">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">Thêm mới loại xe</h4>
                        
                    </div>
                    <div class="card-body">
                        @using (Html.BeginForm("Themmoihang", "Chothue", FormMethod.Post, new { @enctype = "multipart/form-data" }))
                        {
                            @Html.AntiForgeryToken()
                            <div class="row">
                                @Html.ValidationSummary(true)
                                <div class="col-md-6 pl-1">
                                    <div class="form-group">
                                        <label>Loại xe:</label>
                                        @Html.EditorFor(model => model.tenloaiSP, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.tenloaiSP)
                                    </div>
                                </div>
                                <div class="col-md-12 pl-1">
                                    <div class="form-group">
                                        <input type="submit" value="Thêm mới" class="btn btn-info btn-fill pull-right" />
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

